#include<iostream>
using namespace std;

int main() {
	// Write your code here
    char ch;
    cin >> ch;
    if (ch>=65&&ch<=90){
    for(ch='A';ch<='Z';ch++); 
        
        cout << "1" << endl;}
        else if (ch>=97&&ch<=122){
    for(ch='a';ch<='z';ch++);
	 
        cout << "0" << endl;}
        
    else
        cout << "-1" << endl;
    
    
}
